[user]
    name  = Griffin Qiu
    email = griffinqiu@gmail.com
[github]
    user = griffinqiu
[hub]
    protocol = https
[core]
    pager = less -FRSX
    whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
    editor = vim -f
    autocrlf = input
    safecrlf = true
[color]
    diff = auto
    status = auto
    branch = auto
    interactive = auto
    ui = true
    pager = true
[color "branch"]
    current = yellow reverse
    local   = yellow
    remote  = green
[color "diff"]
    meta = blue
    frag = magenta
    old  = red
    new  = green
[color "status"]
    added = yellow
    changed = green
    untracked = blue
[push]
    default = matching
