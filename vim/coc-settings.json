{
  "suggest.autoTrigger": "none",
  "suggest.noselect": false,
  "suggest.triggerAfterInsertEnter": false,
  "suggest.timeout": 500,
  "suggest.detailField": "preview",
  "suggest.triggerCompletionWait": 100,
  "suggest.snippetIndicator": "⭐︎",
  "suggest.echodocSupport": true,
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "\u2A15",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\u2663",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf530",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf6a6",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "diagnostic.signOffset": 9999999,
  "diagnostic.errorSign": "●",
  "diagnostic.warningSign": "●",
  "diagnostic.displayByAle": false,
  "diagnostic.refreshOnInsertMode": true,
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    }
  },
  "list.previewSplitRight": true,
  "list.insertMappings": {
    "<C-c>": "do:exit",
    "<C-d>": "do:stop",
    "<C-t>": "action:tabe",
    "<C-x>": "action:split",
    "<C-v>": "action:vsplit"
  },
  "solargraph.useBundler": true,
  "solargraph.diagnostics": true,
  "solargraph.autoformat": true,
  "solargraph.formatting": true,
  "solargraph.hover": true,
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "vue": ["vue", "javascript"]
  },
  "snippets.editSnippetsCommand": "vsplit",
  "snippets.snipmate.enable": true,
  "snippets.ultisnips.enable": false,
  "snippets.userSnippetsDirectory": "~/.vim/snippets",
  "coc.preferences.jumpCommand": "edit",
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.preferences.snippetStatusText": "Ⓢ ",
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "html",
    "vue",
    "css",
    "javascriptreact"
  ],
  "vetur.format.defaultFormatter.html": "prettier",
  "prettier.statusItemText": "ⓟ ",
  "prettier.eslintIntegration": true,
  "prettier.tslintIntegration": true,
  "prettier.stylelintIntegration": true,
  "eslint.filetypes": ["javascript", "javascriptreact", "vue"],
  "eslint.autoFix": true,
  "eslint.autoFixOnSave": true,
  "python.jediEnabled": false
}
